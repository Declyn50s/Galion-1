generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Immeuble {
  sehl       Int     @id
  adresse    String  @default("")
  baseLegale String  @map("base_legale") @default("")
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}